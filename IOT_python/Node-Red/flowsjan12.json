    {
        "id": "c760813d6866a2ed",
        "type": "function",
        "z": "716670a600e4192d",
        "name": "Temperature_Threshold_Check",
        "func": "const temp_Min = 15;\nconst temp_Max = 30;\n\nif (msg.payload < temp_Min || msg.payload > temp_Max) {\n\n    msg.payload = {\n        chatId: 761490929,\n        type: 'message',\n        content: \"Alarm: The temperature is not within the threshold limit (10째C - 30째C)! Current Temperature:\" + msg.payload +\"째C\"\n    };\n    msg.topic = \"ALARM\";\n    return msg;\n\n} else {\n    msg.topic = \"NORMAL\";\n    msg.payload = {   \n        type: null,\n    content: \"Normal Temperature\"\n    };\n    return msg;\n}",
        "outputs": 1,
